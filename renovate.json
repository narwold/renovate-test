{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "enabledManagers": ["npm", "pip-compile", "poetry", "regex"],
  "dependencyDashboard": true,
  "enabled": true,
  "regexManagers": [
    {
      "fileMatch": ["^myversionsfile.json$"],
      "matchStrings": ["\"node\": \\[(?<prevVersions>.*)\"(?<currentValue>.*?)\"\\]"],
      "autoReplaceStringTemplate": "\"node\": [{{{prevVersions}}}\"{{{currentValue}}}\", \"{{{newValue}}}\"",
      "depNameTemplate": "node",
      "datasourceTemplate": "npm"
    }
  ]
}
